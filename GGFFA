local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local LibraryESP = loadstring(game:HttpGet("https://raw.githubusercontent.com/RGV-devid/Scripts/refs/heads/main/ESP"))()

local Window = Rayfield:CreateWindow({
   Name = "YOUHUB",
   Icon = 95073582077930,
   LoadingTitle = "YOUHUB | Gun Ground FFA",
   LoadingSubtitle = "by rechedmcvn",
   Theme = "Ocean",

   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false,

   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "You hub?"
   },

   KeySystem = true,
   KeySettings = {
      Title = "Key System",
      Subtitle = "Key System",
      Note = "Your Name",
      FileName = "Key",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {game.Players.LocalPlayer.Name}
   }
})

local MainTab = Window:CreateTab("Main", 4483362458)
MainTab:CreateSection("World")
MainTab:CreateToggle({
   Name = "Player Hitbox",
   Callback = function(v)
_G.Hitbox = v
local player = game.Players.LocalPlayer
local targetPart = "HumanoidRootPart"

local function Hitbox()
    local HitboxNil = nil
    for _, otherPlayer in pairs(game.Players:GetPlayers()) do
        if otherPlayer ~= player and otherPlayer.Character and otherPlayer.Character:FindFirstChild(targetPart) then
            otherPlayer.Character.HumanoidRootPart.Size = Vector3.new(30,30,30)
        end
    end
    return HitboxNil
end
local function UnHitbox()
    local UnHitboxNil = nil
    for _, otherPlayer in pairs(game.Players:GetPlayers()) do
        if otherPlayer ~= player and otherPlayer.Character and otherPlayer.Character:FindFirstChild(targetPart) then
            otherPlayer.Character.HumanoidRootPart.Size = Vector3.new(1.5,1.5,1.5)
        end
    end
    return UnHitboxNil
end
game:GetService("RunService").RenderStepped:Connect(function()
    if _G.Hitbox then  
        Hitbox()
    else
        UnHitbox()
    end
  end)
end})
MainTab:CreateSection("Gun")
MainTab:CreateSlider({
   Name = "Reload Time",
   Range = {0, 3},
   Increment = 2,
   CurrentValue = 0.3,
   Callback = function(v)
_G.ReloadTime = v
end})
MainTab:CreateSlider({
   Name = "Fire Rate",
   Range = {0, 1},
   Increment = 2,
   CurrentValue = 0.3,
   Callback = function(v)
_G.FireRate = v
end})
MainTab:CreateSlider({
   Name = "Damage Drop Off",
   Range = {0, 100},
   Increment = 1,
   CurrentValue = 0,
   Callback = function(v)
_G.DamageDropOff = v
end})
MainTab:CreateToggle({
   Name = "Auto Force",
   Callback = function(v)
_G.AutoForce = v
end})
MainTab:CreateToggle({
   Name = "Rapid Shotguns",
   Callback = function(v)
_G.RapidShotguns = not v
end})

MainTab:CreateToggle({
   Name = "Enabled",
   Callback = function(v)
_G.EnabledSettingGun = v
game:GetService("RunService").RenderStepped:Connect(function()
local RunService = game:GetService("RunService")
local players = game:GetService("Players")
local workspace = game:GetService("Workspace")
local plr = players.LocalPlayer
local camera = workspace.CurrentCamera
local mouse = plr:GetMouse()
local hitEffect = "Gib_F"

if _G.EnabledSettingGun and plr.Character then
                local folder = plr.Character:FindFirstChildWhichIsA("Tool")
                
                if folder and folder:FindFirstChild("Configuration") then
                    for _, v in ipairs(folder.Configuration:GetChildren()) do
                        if v.Name == "FireRate" and not e then
                            e = true
                            v.Value = v.Value * _G.FireRate
                        end 
                        if v.Name == "reloadTime" then
                            v.Value = _G.ReloadTime
                        end
                        if v.Name == "DamageDropoff" then
                            v.Value = _G.DamageDropOff
                        end
                        if v.Name == "isAuto" then
                            v.Value = _G.AutoForce
                        end
                        if v.Name == "DelayedShell" then
                            v.Value = _G.RapidShotguns
                        end
                    end
                else
                    e = false
                end
            end
            wait()
        end)
end})
MainTab:CreateSection("ESP")
MainTab:CreateToggle({
   Name = "Players ESP",
   Callback = function(v)
if v then
for _,Player in pairs(game:GetService("Players"):GetPlayers()) do
if Player ~= game:GetService("Players").LocalPlayer and not Player:FindFirstChild("PlayerESP") then
LibraryESP:AddESP(Player.Character, Player.DisplayName .. "\n(@" .. Player.Name .. ")", Color3.new(1,1,1), 14, "PlayerESP")
end
end
PlayersESP = workspace.ChildAdded:Connect(function()
for _,Player in pairs(game:GetService("Players"):GetPlayers()) do
if Player ~= game:GetService("Players").LocalPlayer and not Player:FindFirstChild("PlayerESP") then
LibraryESP:AddESP(Player.Character, Player.DisplayName .. "\n(@" .. Player.Name .. ")", Color3.new(1,1,1), 14, "PlayerESP")
end
end
end)
else
PlayersESP:Disconnect()
end
end})
MainTab:CreateSection("Settings ESP")
MainTab:CreateToggle({
   Name = "Rainbow ESP",
   Callback = function(v)
getgenv().RainbowESP = v
end})
MainTab:CreateToggle({
   Name = "Distance ESP",
   Callback = function(v)
getgenv().DistanceESP = v
end})
MainTab:CreateToggle({
   Name = "Tracer ESP",
   Callback = function(v)
getgenv().TracerESP = v
end})

















