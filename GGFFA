local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local LibraryESP = loadstring(game:HttpGet("https://raw.githubusercontent.com/RGV-devid/Scripts/refs/heads/main/ESP"))()

local Window = Rayfield:CreateWindow({
   Name = "YOUHUB | Gun Ground FFA - v0.0.2",
   Icon = 95073582077930,
   LoadingTitle = "YOUHUB | Gun Ground FFA",
   LoadingSubtitle = "by rechedmcvn",
   Theme = "Ocean",

   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false,

   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "You hub?"
   },

   KeySystem = true,
   KeySettings = {
      Title = "Key System",
      Subtitle = "Key System",
      Note = "Key is 'YOUHUB_[Your Name]'",
      FileName = "Key",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"YOUHUB_" .. game.Players.LocalPlayer.Name}
   }
})

local MainTab = Window:CreateTab("Main", 4483362458)
MainTab:CreateSection("World")
MainTab:CreateToggle({
   Name = "Player Hitbox",
   Callback = function(v)
_G.Hitbox = v
local player = game.Players.LocalPlayer
local targetPart = "HumanoidRootPart"

local function Hitbox()
    local HitboxNil = nil
    for _, otherPlayer in pairs(game.Players:GetPlayers()) do
        if otherPlayer ~= player and otherPlayer.Character and otherPlayer.Character:FindFirstChild(targetPart) then
            otherPlayer.Character.HumanoidRootPart.Size = Vector3.new(30,30,30)
        end
    end
    return HitboxNil
end
local function UnHitbox()
    local UnHitboxNil = nil
    for _, otherPlayer in pairs(game.Players:GetPlayers()) do
        if otherPlayer ~= player and otherPlayer.Character and otherPlayer.Character:FindFirstChild(targetPart) then
            otherPlayer.Character.HumanoidRootPart.Size = Vector3.new(1.5,1.5,1.5)
        end
    end
    return UnHitboxNil
end
game:GetService("RunService").RenderStepped:Connect(function()
    if _G.Hitbox then  
        Hitbox()
    else
        UnHitbox()
    end
  end)
end})
MainTab:CreateSection("Gun")
MainTab:CreateToggle({
   Name = "Fast Reload",
   Callback = function(v)
_G.FastReload = v
game:GetService("RunService").RenderStepped:Connect(function()
if game:GetService("Player").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
if game:GetService("Player").LocalPlayer.Character:FindFirstChild("Configuration") then
for _, v in ipairs(game:GetService("Player").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Configuration:GetChildren()) do
if _G.FastReload and v.Name == "reloadTime" then
v.Value = 0
end
end
end
end
end)
end})
MainTab:CreateToggle({
   Name = "Fast Shot",
   Callback = function(v)
_G.FastShot = v
game:GetService("RunService").RenderStepped:Connect(function()
if game:GetService("Player").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
if game:GetService("Player").LocalPlayer.Character:FindFirstChild("Configuration") then
for _, v in ipairs(game:GetService("Player").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Configuration:GetChildren()) do
if _G.FastShot and v.Name == "FireRate" then
v.Value = 0.3
end
end
end
end
end)
end})
MainTab:CreateToggle({
   Name = "Fast Shot",
   Callback = function(v)
_G.FastShot = v
game:GetService("RunService").RenderStepped:Connect(function()
if game:GetService("Player").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
if game:GetService("Player").LocalPlayer.Character:FindFirstChild("Configuration") then
for _, v in ipairs(game:GetService("Player").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Configuration:GetChildren()) do
if _G.FastShot and v.Name == "FireRate" then
v.Value = 0.3
if _G.FastShot and v.Name == "DamageDropoff" then
v.Value = 9e9
end
end
end
end
end
end)
end})
MainTab:CreateToggle({
   Name = "Auto Force",
   Callback = function(v)
_G.AutoForce = v
game:GetService("RunService").RenderStepped:Connect(function()
if game:GetService("Player").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
if game:GetService("Player").LocalPlayer.Character:FindFirstChild("Configuration") then
for _, v in ipairs(game:GetService("Player").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Configuration:GetChildren()) do
if _G.AutoForce and v.Name == "isAuto" then
v.Value = _G.AutoForce 
end
end
end
end
end)
end})
MainTab:CreateToggle({
   Name = "Rapid Shotguns",
   Callback = function(v)
_G.RapidShotguns = not v
game:GetService("RunService").RenderStepped:Connect(function()
if game:GetService("Player").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
if game:GetService("Player").LocalPlayer.Character:FindFirstChild("Configuration") then
for _, v in ipairs(game:GetService("Player").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Configuration:GetChildren()) do
if not _G.RapidShotguns and v.Name == "DelayedShell" then
v.Value = _G.RapidShotguns
end
end
end
end
end)
end})

















